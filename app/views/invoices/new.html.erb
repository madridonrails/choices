<%= object_messages(@invoice) %>
<h1><span><%= t'invoices.new_invoice' %></span></h1>
<%  form_for @invoice, :url => order_invoice_url(@order), :html => {:class => 'complex-form'} do |f| %>
    <fieldset>
      <legend><%= t 'general.new' %><br/></legend>
      <%= render :partial => 'form', :locals => {:f => f} %>
