<%= object_messages %>
<h1><%= t 'products.products' %></h1>
<% unless @products.empty? %>
  <table id="items-list">
    <%= render :partial => 'header' %>
    <%= render :partial => 'product', :collection => @products %>
  </table>
<% end %>
<%=  link_to image_tag('plus_32.png', :class => 'add-button', :alt => t('general.new'), :title => t('general.new')), new_product_url %>
<%= will_paginate(@products, :prev_label => t('pagination.prev'), :next_label => t('pagination.next'), :params => params.clone.delete_if{|k,v| k == 'controller' || k == 'action' || k == 'page'}) %>
