<ol class="span-22">
  <li class="span-11">
    <%= f.label :code, t('activerecord.attributes.delivery_note.code'), :class=>'span-3' %>
    <%= f.text_field :code, :class=>'span-5'%>
  </li>
  <li class="span-11">
    <%= f.label :packages, t('activerecord.attributes.delivery_note.packages'), :class=>'span-3' %>
    <%= f.text_field :packages, :class=>'span-5'%>
  </li>
  <li>
    <button type="submit" class="positive">
      <%= image_tag 'icons/tick.png', :alt => t('general.ok'), :title => t('general.ok') %><span><%= t'general.ok' %></span>
    </button>
    <%= link_to image_tag('icons/cross.png', :alt => t('general.cancel')) + (t'general.back'), orders_path, :class => 'button negative', :title => t('general.cancel') %>
    <%= link_to image_tag('print_32.png', :alt => t('general.print')) + (t'general.print'), print_url, :target => :blank, :class => 'button', :title => t('general.print') unless @delivery_note.new_record?%>

  </li>

</ol>
